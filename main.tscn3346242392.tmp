resource_name = "Blinker"
script/source = "extends AnimatedSprite2D


@onready var blink_timer: Timer = $BlinkTimer


func _on_blink_timer_timeout() -> void:
	play(\"blink\")
	blink_timer.wait_time = randi_range(3, 8)
	blink_timer.start()
"

